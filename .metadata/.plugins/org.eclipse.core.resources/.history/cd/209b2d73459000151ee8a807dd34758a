import com.modeliosoft.modelio.javadesigner.annotations.objid;

@objid ("195a4b26-babf-433c-875f-aace18716e69")
public abstract class Voiture {
    
    public String nom;

    
    public float masse;

    
    public int numero;

    
    public abstract int prix();

    
    public Voiture() {
    	super();
    }

    
    public Voiture(String nom) {
    	super();
    	this.nom=nom;
    }

   
	public String toString() {
		return "Voiture [nom=" + nom + ", masse=" + masse + ", numero=" + numero + "]";
	}

}


public abstract class Voiture
{
	@objid ("cc0e0962-6cf2-4eb2-8ebc-e60cc5148ebd")
	private String nom ;
	
	@objid ("f5a106eb-e43d-417b-8782-5db9ca213e95")
	private int numero ;
	
	@objid ("d5727ba1-761e-4d29-80b7-d0bfa892ff22")
	private float masse ;
	
	
	@objid ("cf11b273-4c68-4e7b-928e-a850eb3b6a9e")
	public Voiture()
	{
	}
	
	/**
	 *
	 */
	public Voiture(Voiture v)
	{
		this.modele = v.getModele() ;
		this.numero = v.getNumero() ;
		this.masseVoiture = v.getMasse() ;
	}
	
	@objid ("7f3559f8-39cd-400f-b79f-12610c64efdd")
	public Voiture(int unNumero, String unModele, float uneMasseVoiture)
	{
		this.numero = unNumero ;
		this.modele = unModele ;
		this.masseVoiture = uneMasseVoiture ;
	}
	
	
	@objid ("e832b274-d9ba-421a-82ab-c6222e5ca77c")
	public abstract int prix() ;
	
	
	/**
	 * retourne le numero de la voiture
	 */
	public int getNumero()
	{
		return this.numero ;
	}
	
	
	/**
	 * retourne le modele de la voiture
	 */
	public String getModele()
	{
		return this.modele ;
	}
	/**
	 * retourne la masse de la voiture	*/
	public float getMasse()
	{
		return this.masseVoiture ;
	}
	
	@objid ("03739065-a4f5-4bf8-adcc-a044d354f20d")
	@Override
	public String toString()
	{
		return "modele = " + this.getModele() + " n°" + this.getNumero() + " poids = " + this.getMasse() + "t (" +	this.prix() + "€)" ;
	}
	
}
// fin classe Voiture
